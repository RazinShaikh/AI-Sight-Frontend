<?xml version='1.0' encoding='utf-8'?>
<widget id="com.unnc.grp1718.aisight" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>AI-Sight</name>
    <description>
        AI-Sight, A simple object detection app.
    </description>
    <author>
        UNNC GRP Team 3 17-18
    </author>
    <icon src="icon.png" />
    <content src="index.html" />
    <access origin="*" />
    <plugin name="cordova-plugin-camera" spec="^4.0.2" />
    <plugin name="cordova-plugin-console" spec="~1.1.0" />
    <plugin name="cordova-plugin-statusbar" spec="~2.3.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="~1.7.0" />
    <plugin name="cordova-plugin-file" spec="~5.0.0" />
    <plugin name="cordova-plugin-tts" spec="^0.2.3" />
    <plugin name="cordova-plugin-camera-preview" spec="^0.9.0" />
    <preference name="Orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <engine name="browser" spec="^5.0.3" />
    <engine name="android" spec="^7.0.0" />
</widget>
